            { name: 'Full lapis emas 24k tahan lama di jamin Gelang Bangle Lapis Emas Jumbo', price: 120000 },
            { name: 'Kalung Lapis Emas 24K Susun 3 Model bunga lingkaran sudah di sepuh kembali', price: 190000 },
