            { name: 'Product Name 4', price: 250000 },
            { name: 'Product Name 5', price: 300000 },
            { name: 'Product Name 2', price: 150000 },
            { name: 'Product Name 3', price: 200000 },
            { name: 'Product Name 4', price: 250000 },
            { name: 'Product Name 5', price: 300000 },
        ];

        function addToCart(productName, price) {
            cart.push({ productName, price });
            alert(`${productName} telah ditambahkan ke keranjang.`);
            updateCart();
            document.getElementById('address-form').classList.remove('hidden');
            document.getElementById('cart-section').scrollIntoView({ behavior: 'smooth' });
        }

        function removeFromCart(index) {
            cart.splice(index, 1);
            updateCart();
        }

        function updateCart() {
            const cartContainer = document.getElementById('cart-container');
            cartContainer.innerHTML = '';
            let total = 0;
            cart.forEach((item, index) => {
                const cartItem = document.createElement('div');
                cartItem.className = 'flex justify-between items-center p-2 border-b';
                cartItem.innerHTML = `
                    <span>${item.productName}</span>
                    <span>Rp ${item.price}</span>
                    <button class="text-red-500" onclick="removeFromCart(${index})">Hapus</button>
                `;
                cartContainer.appendChild(cartItem);
                total += item.price;
            });

            const shippingInfo = document.createElement('div');
            shippingInfo.className = 'flex justify-between items-center p-2 border-t mt-4';
            shippingInfo.innerHTML = `
                <span>Ongkir</span>
                <span>Rp 15.000</span>
            `;
            cartContainer.appendChild(shippingInfo);

            total += 15000; // Adding shipping cost

            const totalInfo = document.createElement('div');
            totalInfo.className = 'flex justify-between items-center p-2 border-t mt-4';
            totalInfo.innerHTML = `
                <span>Total Belanja</span>
                <span>Rp ${total}</span>
            `;
            cartContainer.appendChild(totalInfo);
        }

        function showCart() {
            document.getElementById('cart-section').scrollIntoView({ behavior: 'smooth' });
        }

        function sendToWhatsApp() {
            const name = document.getElementById('name').value;
            const province = document.getElementById('province').value;
            const regency = document.getElementById('regency').value;
            const city = document.getElementById('city').value;
            const street = document.getElementById('street').value;
            const houseDetails = document.getElementById('house-details').value;

            let cartDetails = cart.map(item => `${item.productName} - Rp ${item.price}`).join('\n');
            let total = cart.reduce((sum, item) => sum + item.price, 0) + 15000;

            const message = `Nama: ${name}\nProvinsi: ${province}\nKabupaten: ${regency}\nKota: ${city}\nNama Jalan: ${street}\nDetail Rumah: ${houseDetails}\n\nKeranjang:\n${cartDetails}\n\nOngkir: Rp 15.000\nTotal Belanja: Rp ${total}\n\nKirim Nota COD ini nah kak, biar ku proses Pengataran ta' 4-5 hari, kami di usahakan 3 hari sampai kakü´∂üèª`;

            const whatsappUrl = `https://wa.me/+6289630920692?text=${encodeURIComponent(message)}`;
            window.open(whatsappUrl, '_blank');
        }

        window.alert = function(message) {
            const alertBox = document.createElement('div');
            alertBox.className = 'fixed top-0 left-0 w-full h-full flex items-center justify-center bg-black bg-opacity-50';
            alertBox.innerHTML = `
                <div class="bg-white p-4 rounded shadow-lg text-center">
                    <p class="mb-4">${message}</p>
                    <button class="bg-orange-600 text-white px-4 py-2 rounded" onclick="this.parentElement.parentElement.remove()">OK</button>
                </div>
            `;
            document.body.appendChild(alertBox);
        };

        function zoomImage(src) {
            const overlay = document.createElement('div');
            overlay.className = 'overlay';
            overlay.onclick = function() {
                document.body.removeChild(overlay);
                document.body.removeChild(zoomedImage);
            };

            const zoomedImage = document.createElement('img');
            zoomedImage.src = src;
            zoomedImage.className = 'zoomed';

            document.body.appendChild(overlay);
            document.body.appendChild(zoomedImage);
        }
    </script>
